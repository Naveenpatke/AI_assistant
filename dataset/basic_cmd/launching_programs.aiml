<aiml version="1.0.1" encoding="UTF-8">
	
 <!-- Launching programs -->

    <category>
        <pattern>OPEN * CODE</pattern>
         <template>
             <random>
               <li>Sure thing!</li>
               <li>Right away, sir!</li>
               <li>On it!</li>
            </random>
            <system>code</system>
        </template>
    </category>

    <category>
        <pattern>* OPEN * CODE</pattern>
         <template>
            <srai>OPEN CODE</srai>
        </template>
    </category>

    <category>
        <pattern>CLOSE * CODE</pattern>
         <template>
             <random>
               <li>Sure thing!</li>
               <li>Right away, sir!</li>
               <li>On it!</li>
            </random>
            <system>tskill code</system>
        </template>
    </category>

    <category>
        <pattern>* CLOSE * CODE</pattern>
         <template>
            <srai>CLOSE * CODE</srai>
        </template>
    </category>

